# Define the request
$uri = "http://your-server/"
$headers = @{
    "Content-Type" = "application/json"
    "X-JWT-Token" = "your_valid_jwt_token"
}
$body = @{
    model = "llama-70b"
    prompt = "Hello"
} | ConvertTo-Json

# Send the request and capture response
$response = Invoke-WebRequest -Uri $uri -Method Post -Headers $headers -Body $body -Verbose

# Display all response headers
$response.Headers
