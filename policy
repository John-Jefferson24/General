apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: jupyterhub-projects-internal-only
  labels:
    app: jupyterhub-genai
    name: jupyterhub-genai
    template: jupyterhub-genai
spec:
  podSelector:
    matchExpressions:
    - key: project
      operator: In
      values: ["reverse-dictionary", "ffep", "bbg"]
  egress:
  - to:
    - namespaceSelector:
        matchLabels:
          kubernetes.io/metadata.name: 74388-confl
  policyTypes:
  - Egress
