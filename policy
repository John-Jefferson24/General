apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: jupyterhub-projects-internal-only
  labels:
    app: jupyterhub-genai
    name: jupyterhub-genai
    template: jupyterhub-genai
spec:
  podSelector:
    matchExpressions:
    - key: project
      operator: In
      values: ["reverse-dictionary", "ffep", "bbg"]
  egress:
  - to:
    - podSelector:
        matchLabels:
          app: pypiserver
  - to:
    - podSelector:
        matchExpressions:
        - key: project
          operator: Exists
  policyTypes:
  - Egress
